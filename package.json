{
  "name": "backbone-reactivity-adapter",
  "version": "0.1.0",
  "description": "An adapter using ES6 Proxies to adapt Backbone Models to Javascript Objects",
  "scripts": {
    "postinstall": "lerna bootstrap --hoist && link-parent-bin",
    "build": "lerna run build",
    "clean": "lerna clean --yes",
    "postclean": "rm -rf node_modules",
    "cleanBuilds": "lerna exec -- rm -rf ./dist",
    "test": "jest"
  },
  "author": "Bill Glesias",
  "license": "MIT",
  "engines": {
    "node": ">= 12.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.11",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-html-reporter": "^3.1.3",
    "lerna": "^3.22.1",
    "link-parent-bin": "^1.0.2",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {}
}
